{
  "name": "css-putter",
  "version": "0.0.2",
  "description": "css-putter apply CSS file on a web page at a time, whenever you hope yourself timing.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "format": "prettier --write .",
    "test": "jest --silent",
    "prepublish": "tsc"
  },
  "keywords": [
    "css",
    "inject css",
    "insert css"
  ],
  "author": "ikmnjrd",
  "homepage": "https://github.com/ikmnjrd/css-putter",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ikmnjrd/css-putter.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.1.1",
    "jest": "^29.1.2",
    "jest-environment-jsdom": "^29.1.2",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.3"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "preset": "ts-jest",
    "transform": {
      "^.+\\.tsx?$": [
        "ts-jest",
        {
          "ts-jest": {
            "tsconfig": "tests/tsconfig.json"
          }
        }
      ]
    }
  }
}
